{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "nome": {
      "type": "string"
    },
    "descricao": {
      "type": "string"
    },
    "valor": {
      "type": "number"
    },
    "categoria": {
      "type": "string"
    },
    "imagemUrl": {
      "type": "string"
    }
  },
  "required": ["id", "nome", "descricao", "valor", "categoria", "imagemUrl"]
}